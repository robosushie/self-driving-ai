# This .gitignore file should be placed at the root of the parent directory
#
# Get latest from https://github.com/github/gitignore/blob/main/Unity.gitignore
#
/unity/*/[Ll]ibrary/
/unity/*/[Tt]emp/
/unity/*/[Oo]bj/
/unity/*/[Bb]uild/
/unity/*/[Bb]uilds/
/unity/*/[Ll]ogs/
/unity/*/[Uu]ser[Ss]ettings/

# MemoryCaptures can get excessive in size.
# They also could contain extremely sensitive data
/unity/*/[Mm]emoryCaptures/

# Recordings can get excessive in size
/unity/*/[Rr]ecordings/

# Uncomment this line if you wish to ignore the asset store tools plugin
# /unity/*/[Aa]ssets/AssetStoreTools*

# Autogenerated Jetbrains Rider plugin
/unity/*/[Aa]ssets/Plugins/Editor/JetBrains*

# Visual Studio cache directory
/unity/*/.vs/

# Gradle cache directory
/unity/*/.gradle/

# Autogenerated VS/MD/Consulo solution and project files
/unity/*/ExportedObj/
/unity/*/.consulo/
/unity/*/*.csproj
/unity/*/*.unityproj
/unity/*/*.sln
/unity/*/*.suo
/unity/*/*.tmp
/unity/*/*.user
/unity/*/*.userprefs
/unity/*/*.pidb
/unity/*/*.booproj
/unity/*/*.svd
/unity/*/*.pdb
/unity/*/*.mdb
/unity/*/*.opendb
/unity/*/*.VC.db

# Unity3D generated meta files
/unity/*/*.pidb.meta
/unity/*/*.pdb.meta
/unity/*/*.mdb.meta

# Unity3D generated file on crash reports
/unity/*/sysinfo.txt

# Builds
/unity/*/*.apk
/unity/*/*.aab
/unity/*/*.unitypackage
/unity/*/*.app

# Crashlytics generated file
/unity/*/crashlytics-build.properties

# Packed Addressables
/unity/*/Assets/AddressableAssetsData/*/*.bin*

# Temporary auto-generated Android Assets
/unity/*/Assets/StreamingAssets/aa.meta
/unity/*/Assets/StreamingAssets/aa/*
